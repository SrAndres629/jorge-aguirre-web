{
    "version": 2,
    "name": "jorge-aguirre-web",
    "builds": [
        {
            "src": "web/api/index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        },
        {
            "src": "web/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/web/static/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "/web/static/favicon.svg"
        },
        {
            "src": "/(.*)",
            "dest": "/web/api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": "web"
    },
    "regions": [
        "iad1"
    ],
    "github": {
        "silent": true
    }
}